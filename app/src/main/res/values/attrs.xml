<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="TextViewFont">
        <attr name="customFont" format="string" />
    </declare-styleable>

    <declare-styleable name="AvatarView">
        <attr name="avatarType" format="enum">
            <enum name="live" value="0" />
            <enum name="regular" value="1" />
        </attr>
    </declare-styleable>

    <declare-styleable name="AvatarLiveView">
        <attr name="avatarLiveType" format="enum">
            <enum name="live" value="0" />
            <enum name="connected" value="1" />
        </attr>
    </declare-styleable>

    <declare-styleable name="RoundedCornerLayout">
        <attr name="cornerRadius" format="dimension" />
    </declare-styleable>

    <declare-styleable name="TileView">
        <attr name="tileType" format="enum">
            <enum name="live" value="0" />
            <enum name="connected" value="1" />
            <enum name="invite" value="2" />
            <enum name="grid" value="3" />
        </attr>
    </declare-styleable>

    <declare-styleable name="PictoVisualizerView">
        <attr name="picto" format="integer" />
    </declare-styleable>

    <declare-styleable name="LabelButton">
        <attr name="drawable" format="integer" />
        <attr name="type" format="enum">
            <enum name="infos" value="0" />
            <enum name="action" value="1" />
        </attr>
        <attr name="label" format="string" />
    </declare-styleable>

    <declare-styleable name="CameraWrapper">
        <attr name="cameraGradient" format="boolean" />
        <attr name="cameraType" format="enum">
            <enum name="recording" value="0" />
            <enum name="setting" value="1" />
        </attr>
        <attr name="cameraHeight" format="dimension" />
        <attr name="cameraWidth" format="dimension" />
    </declare-styleable>

    <declare-styleable name="TribeVideoView">
        <attr name="scaleVideoType" format="enum">
            <enum name="top" value="0" />
            <enum name="bottom" value="1" />
            <enum name="fill" value="2" />
            <enum name="center_crop" value="3" />
            <enum name="center_crop_fill" value="4" />
        </attr>
        <attr name="autoStart" format="boolean" />
        <attr name="looping" format="boolean" />
        <attr name="mute" format="boolean" />
        <attr name="speedControl" format="boolean" />
    </declare-styleable>

    <declare-styleable name="ButtonPointsView">
        <attr name="buttonDrawable" format="integer" />
        <attr name="buttonType" format="enum">
            <enum name="profile" value="0" />
            <enum name="fb_active" value="1" />
            <enum name="fb_progress" value="2" />
            <enum name="fb_disabled" value="3" />
        </attr>
        <attr name="buttonLabel" format="string" />
        <attr name="buttonSubLabel" format="string" />
        <attr name="buttonPoints" format="integer" />
        <attr name="buttonBGColor" format="integer" />
    </declare-styleable>

    <declare-styleable name="ButtonCardView">
        <attr name="buttonCardDrawable" format="integer" />
        <attr name="buttonCardType" format="enum">
            <enum name="points" value="0" />
            <enum name="meteo" value="1" />
            <enum name="location" value="2" />
            <enum name="city" value="3" />
        </attr>
    </declare-styleable>

    <declare-styleable name="TextViewAnimatedDots">
        <attr name="textStyle" format="integer" />
    </declare-styleable>

    <declare-styleable name="GroupSuggestionView">
        <attr name="groupDrawable" format="integer" />
        <attr name="groupType" format="enum">
            <enum name="bestfriends" value="0" />
            <enum name="teammates" value="1" />
            <enum name="classmates" value="2" />
            <enum name="roomies" value="3" />
            <enum name="workteam" value="4" />
            <enum name="family" value="5" />
        </attr>
    </declare-styleable>

    <declare-styleable name="ActionView">
        <attr name="actionTitle" format="string" />
        <attr name="actionBody" format="string" />
        <attr name="actionType" format="enum">
            <enum name="hierarchy" value="0" />
            <enum name="hierarchy_with_image" value="1" />
            <enum name="sharing" value="2" />
            <enum name="toggle" value="3" />
            <enum name="critical" value="4" />
            <enum name="regular" value="5" />
        </attr>
    </declare-styleable>

    <declare-styleable name="SyncView">
        <attr name="syncIcon" format="integer" />
        <attr name="syncBackground" format="integer" />
        <attr name="syncType" format="enum">â€¡
            <enum name="fb" value="0" />
            <enum name="addressbook" value="1" />
        </attr>
    </declare-styleable>

    <declare-styleable name="LoadFriendsView">
        <attr name="loadTitle" format="string" />
        <attr name="loadBody" format="string" />
        <attr name="loadBG" format="integer" />
        <attr name="loadIcon" format="integer" />
        <attr name="loadType" format="enum">
            <enum name="fb" value="0" />
            <enum name="addressbook" value="1" />
        </attr>
    </declare-styleable>

    <declare-styleable name="LiveIndView">
        <attr name="liveType" format="enum">
            <enum name="live" value="0" />
            <enum name="connected" value="1" />
        </attr>
    </declare-styleable>

    <declare-styleable name="PickAllView">
        <attr name="pickAllTitle" format="string" />
        <attr name="pickAllBody" format="string" />
        <attr name="pickAllBGColor" format="color" />
    </declare-styleable>

    <declare-styleable name="PulseLayout">
        <attr name="pulseCount" format="integer" />
        <attr name="pulseDuration" format="integer" />
        <attr name="pulseColor" format="color" />
    </declare-styleable>

    <declare-styleable name="LiveNotificationView">
        <attr name="notificationTitle" format="string" />
    </declare-styleable>
</resources>
  <string name="messagefragment.info" translatable="false">
        fragment MessagePayload on MessageInterface {
           __typename
            id
            ... on EmojiMessage {
              author {
                display_name
                id
                picture
              }
              data
            }
            ... on TextMessage {
              author {
                display_name
                id
                picture
              }
              data
            }
            ... on ImageMessage {
              author {
                display_name
                id
                picture
              }
              original {
                url
                filesize
                width
                height
              }
              alts {
                url
                filesize
                width
                height
              }
            }
            ... on EventMessage {
                  id
                  action
                  user{id picture display_name}
                  created_at
            }
           ... on PokeMessage {
            author {
                display_name
                id
                picture
              }
                  client_message_id
                  intent
                  game_id
                  data
            }
          ... on AudioMessage {
                author {
                  display_name
                  id
                  picture
                }
                original {
                  url
                  duration
                  filesize
                }
                alts {
                  url
                  duration
                  filesize
                }
                created_at
            }
            created_at
          }
  </string>


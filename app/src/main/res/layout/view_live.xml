<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android"
    >

  <com.tribe.app.presentation.view.component.live.LiveRoomView
      android:id="@+id/viewRoom"
      android:layout_width="match_parent"
      android:layout_height="match_parent"
      android:animateLayoutChanges="true"
      >

  </com.tribe.app.presentation.view.component.live.LiveRoomView>

  <com.tribe.app.presentation.view.component.live.LiveControlsView
      android:id="@+id/viewControlsLive"
      android:layout_width="match_parent"
      android:layout_height="match_parent"
      android:layout_gravity="bottom"
      />

  <com.tribe.app.presentation.view.component.live.LiveStatusNameView
      android:id="@+id/viewStatusName"
      android:layout_width="match_parent"
      android:layout_height="100dp"
      android:layout_gravity="top|center_horizontal"
      android:layout_marginLeft="50dp"
      android:layout_marginRight="50dp"
      android:layout_marginTop="19.5dp"
      android:background="@null"
      />

</merge>
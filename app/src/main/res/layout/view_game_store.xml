<?xml version="1.0" encoding="utf-8"?>
<com.tribe.app.presentation.view.component.games.GamesStoreView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@null"
    android:fillViewport="true"
    tools:context=".presentation.view.activity.NewGameActivity"
    >

  <android.support.v7.widget.RecyclerView
      android:id="@+id/recyclerViewGames"
      android:layout_width="match_parent"
      android:layout_height="match_parent"
      android:background="@null"
      android:fadingEdgeLength="0dp"
      android:overScrollMode="never"
      android:requiresFadingEdge="none"
      android:scrollbars="none"
      />

</com.tribe.app.presentation.view.component.games.GamesStoreView>
<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent" android:layout_height="@dimen/bottom_bar_with_shadow_height">

    <View
        android:layout_width="match_parent"
        android:layout_height="10dp"
        android:background="@drawable/shadow_top"
        />

    <com.tribe.app.presentation.view.widget.TextViewFont
        android:id="@+id/txtAccessTry"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_gravity="bottom|center_horizontal"
        android:layout_marginTop="10dp"
        android:background="@drawable/shape_rect_blue_rounded_bottom"
        android:text="@string/onboarding_queue_button_title"
        android:gravity="center"
        android:clickable="true"
        android:foreground="@drawable/selectable_button_light"
        style="@style/Headline.White.2"
        />

</FrameLayout>
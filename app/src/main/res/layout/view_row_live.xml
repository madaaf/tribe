<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android">

  <com.tribe.app.presentation.view.component.live.LiveWaitingView
      android:id="@+id/viewWaiting"
      android:layout_width="match_parent"
      android:layout_height="match_parent"
      android:layout_gravity="center"
      />

  <LinearLayout
      android:id="@+id/layoutStream"
      android:layout_width="match_parent"
      android:layout_height="match_parent"
      android:orientation="horizontal"
      android:visibility="gone"
      >

  </LinearLayout>

  <com.tribe.app.presentation.view.component.live.LivePeerOverlayView
      android:id="@+id/viewPeerOverlay"
      android:layout_width="match_parent"
      android:layout_height="match_parent"
      android:layout_gravity="center"
      android:visibility="gone"
      />

  <View
      android:id="@+id/viewBackground"
      android:layout_width="match_parent"
      android:layout_height="match_parent"
      android:background="@android:color/black"
      android:visibility="gone"
      />

  <ImageView
      android:id="@+id/addFriend"
      android:layout_width="40dp"
      android:layout_height="40dp"
      android:layout_gravity="center"
      android:clickable="true"
      android:src="@drawable/picto_add_friend"
      android:visibility="gone"
      />

</merge>